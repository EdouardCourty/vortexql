{
    "env": {
        "browser": false,
        "es2021": true,
        "node": true,
        "commonjs": false
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-prototype-builtins": "off"
    }
}
